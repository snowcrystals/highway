{
	"name": "@snowcrystals/highway",
	"version": "1.1.2",
	"description": "Building API routes has never been easier 🛣️",
	"author": "ijsKoud <github@ijskoud.dev>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs",
		"types": "./dist/index.d.ts"
	},
	"imports": {
		"#types/*": "dist/lib/types/*",
		"#structures/*": "dist/lib/structures/*",
		"#handlers/*": "dist/lib/handlers/*"
	},
	"files": [
		"dist/"
	],
	"homepage": "https://github.com/snowcrystals/highway",
	"scripts": {
		"build": "tsup src",
		"build:watch": "tsc --watch",
		"start:dev": "nodemon ./dist",
		"start": "node ./dist",
		"lint": "eslint src",
		"postinstall": "is-ci || husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"bump": "cliff-jumper",
		"docs": "typedoc-json-parser",
		"test": "vitest run"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-angular": "^17.7.0",
		"@favware/cliff-jumper": "^2.1.2",
		"@sapphire/eslint-config": "^5.0.2",
		"@sapphire/ts-config": "^4.0.1",
		"@snowcrystals/prettier-config": "^1.0.0",
		"@types/express": "^4.17.17",
		"@types/node": "^18.17.15",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^14.0.1",
		"nodemon": "^3.0.1",
		"pinst": "^3.0.0",
		"prettier": "^3.0.3",
		"tsup": "^7.2.0",
		"typedoc": "^0.25.1",
		"typedoc-json-parser": "^8.2.0",
		"typescript": "5.2.2",
		"vitest": "^0.34.4"
	},
	"dependencies": {
		"@discordjs/collection": "^1.5.3",
		"express": "^4.18.2",
		"express-rate-limit": "^6.11.0",
		"glob": "^10.3.4"
	},
	"engines": {
		"node": ">= v18.16.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.6.3"
}
